{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MatchReport",
  "type": "object",
  "required": [
    "ask_exists",
    "ask_id",
    "bid_exists",
    "bid_id",
    "match_possible"
  ],
  "properties": {
    "ask_exists": {
      "type": "boolean"
    },
    "ask_id": {
      "type": "string"
    },
    "bid_exists": {
      "type": "boolean"
    },
    "bid_id": {
      "type": "string"
    },
    "error_message": {
      "type": [
        "string",
        "null"
      ]
    },
    "match_possible": {
      "type": "boolean"
    }
  }
}
